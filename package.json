{
  "name": "i18n-auto",
  "displayName": "i18n-auto",
  "description": "",
  "author": "ruo-love",
  "publisher": "ruo-love",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "vsce": {
    "out": "vsix-out"
  },
  "contributes": {
    "commands": [
      {
        "command": "i18n-auto.translate",
        "title": "i18n-auto"
      }
    ],
    "keybindings": {
      "command": "i18n-auto.translate",
      "key": "Ctrl+2"
    },
    "menus": {
      "editor/context": [
        {
          "command": "i18n-auto.translate",
          "when": "editorTextFocus"
        }
      ]
    },
    "icon": "icon.png",
    "configuration": {
      "type": "object",
      "title": "translate configuration",
      "properties": {
        "translate.toLang": {
          "type": "string",
          "enum": [
            "中文",
            "日文",
            "英文",
            "韩文",
            "法文",
            "俄文",
            "葡萄牙文",
            "西班牙文",
            "越南文"
          ],
          "default": "英文",
          "description": "目标语言"
        },
        "translate.select": {
          "type": "string",
          "enum": [
            "selected",
            "row"
          ],
          "default": "selected",
          "description": "内容抓取方式"
        },
        "translate.tool": {
          "type": "string",
          "enum": [
            "有道",
            "百度"
          ],
          "default": "有道",
          "description": "选择翻译服务平台"
        },
        "translate.youdao.appKey": {
          "type": "string",
          "default": "NjeTPKD8wMidwDNSE4zr",
          "description": "有道翻译appKey"
        },
        "translate.youdao.appSecret": {
          "type": "string",
          "default": "3qElbjXX1P8o5drSPoF17db4Y1LKWhH3",
          "description": "有道翻译appSecret"
        },
        "translate.baidu.appKey": {
          "type": "string",
          "default": "20240924002159392",
          "description": "百度翻译appKey"
        },
        "translate.baidu.appSecret": {
          "type": "string",
          "default": "NjeTPKD8wMidwDNSE4zr",
          "description": "百度翻译appSecret"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "uuid": "^10.0.0"
  }
}